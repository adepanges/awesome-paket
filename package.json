{
    "name": "awesome-paket",
    "version": "1.0.0",
    "description": "",
    "main": "src/index.js",
    "scripts": {
        "start": "npm run start:prod",
        "start:prod": "cross-env NODE_ENV=production node dist/server.bootstrap.js",
        "start:dev": "nodemon --watch \"src/**/*.ts\" --ignore \"node_modules/**/*\" --exec NODE_ENV=development ts-node src/server.bootstrap.ts",
        "build": "rm -rfd dist && npm run build-ts && npm run lint",
        "clean": "rimraf '{src,test}/**/*.{js,js.map}'",
        "build-ts": "tsc --project tsconfig.json",
        "build-ts:w": "tsc --project tsconfig.json -w",
        "test": "npm clean && npm test:lint && npm test:coverage",
        "test:unit": "cross-env NODE_ENV=test mocha",
        "test:coverage": "cross-env NODE_ENV=test nyc mocha",
        "test:lint": "tslint --project tsconfig.json",
        "test:lint:fix": "tslint --project tsconfig.json --fix",
        "lint": "tsc --noEmit && eslint \"**/*.{js,ts}\" --quiet --fix"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/adepanges/awesome-paket.git"
    },
    "author": "Ade Pangestu",
    "license": "ISC",
    "bugs": {
        "url": "https://github.com/adepanges/awesome-paket/issues"
    },
    "homepage": "https://github.com/adepanges/awesome-paket#readme",
    "devDependencies": {
        "@types/bluebird": "^3.5.33",
        "@types/chai": "^4.2.14",
        "@types/chai-as-promised": "^7.1.3",
        "@types/compression": "^1.7.0",
        "@types/cors": "^2.8.9",
        "@types/express": "^4.17.9",
        "@types/jest": "^26.0.19",
        "@types/method-override": "0.0.31",
        "@types/mocha": "^8.2.0",
        "@types/multer": "^1.4.5",
        "@types/node": "^14.14.20",
        "@typescript-eslint/eslint-plugin": "^2.3.1",
        "@typescript-eslint/parser": "^2.3.1",
        "chai": "^4.2.0",
        "chai-as-promised": "^7.1.1",
        "eslint": "^7.17.0",
        "eslint-config-prettier": "^6.15.0",
        "eslint-plugin-prettier": "^3.1.4",
        "mocha": "^8.2.1",
        "nyc": "^15.1.0",
        "prettier": "^2.1.2",
        "sinon-chai": "^3.5.0",
        "ts-node": "^9.1.1",
        "tsconfig-paths": "^3.9.0",
        "typescript": "^4.1.3"
    },
    "dependencies": {
        "@tsed/ajv": "^6.19.1",
        "@tsed/common": "^6.19.1",
        "@tsed/core": "^6.19.1",
        "@tsed/di": "^6.19.1",
        "@tsed/exceptions": "^6.19.1",
        "@tsed/json-mapper": "^6.19.1",
        "@tsed/mongoose": "^6.19.1",
        "@tsed/platform-express": "^6.19.1",
        "@tsed/schema": "^6.19.1",
        "@tsed/swagger": "^6.19.1",
        "@tsed/testing-mongoose": "^6.19.1",
        "@types/sinon": "^9.0.10",
        "cross-env": "^7.0.3",
        "bluebird": "^3.7.2",
        "class-transformer": "^0.3.1",
        "class-validator": "^0.12.2",
        "compression": "^1.7.4",
        "cors": "^2.8.5",
        "dotenv": "^8.2.0",
        "express": "^4.17.1",
        "fs": "0.0.1-security",
        "helmet": "^4.3.1",
